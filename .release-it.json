{
  "git": {
    "commitMessage": "Version ${version}",
    "tagAnnotation": "Version ${version}",
    "pushRepo": "origin"
  },
  "npm": {
    "publish": false
  },
  "github": {
    "release": true,
    "releaseName": "Version ${version}"
  },
  "hooks": {
    "after:bump": "npm run sonar:properties"
  },
  "plugins": {
    "@release-it/keep-a-changelog": {
      "filename": "CHANGELOG.md",
      "head": "main",
      "addVersionUrl": true
    }
  }
}
